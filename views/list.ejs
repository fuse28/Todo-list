<%- include("header"); -%>  


    <div class="box" id="heading">
      <h1 >Today is <%= ListTitle %></h1>
    </div>
   <div class="box">
    

      <%  for(let i=0; i<newLists.length; i++) { %>
        <div class="item">
          <input type="checkbox" >
          <p><%=newLists[i]%></p>
        </div>
         
        <%}%>
        <form class="item" action="/" method="post">
          <input type="text" name="newItem" placeholder="Enter list" required autocomplete="off" />
          <button type="submit" name="list" value=<%= ListTitle %> >+</button>
         
        </form>
   </div>

   <%- include("footer"); -%> 
   
  </body>
</html>
